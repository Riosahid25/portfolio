  "main": "index.js",
  "scripts": {
    "build:profile_data": "npx tailwindcss -i ./src/profile_data.css -o ./output/output_profile_data.css --watch",
    "build:profile_info": "npx tailwindcss -i ./src/profile_info.css -o ./output/output_profile_info.css --watch",
    "build:profile_buttons": "npx tailwindcss -i ./src/profile_buttons.css -o ./output/output_profile_buttons.css --watch",
    "build:main": "npx tailwindcss -i ./src/main.css -o ./output/output_main.css --watch",
    "build:filter_content": "npx tailwindcss -i ./src/filter_content.css -o ./output/output_filter_content.css --watch",
    "build:filter_section": "npx tailwindcss -i ./src/filter_section.css -o ./output/output_filter_section.css --watch",
    "build:footer": "npx tailwindcss -i ./src/footer.css -o ./output/output_footer.css --watch",
    "build:all": "concurrently \"npm run build:profile_data\" \"npm run build:profile_info\" \"npm run build:profile_buttons\" \"npm run build:main\" \"npm run build:filter_content\" \"npm run build:filter_section\" \"npm run build:footer\""
  },
